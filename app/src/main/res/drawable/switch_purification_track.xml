<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!--<item android:state_checked="true"-->
    <!--android:drawable="@drawable/back_switch_b"></item>-->
    <!--<item android:state_checked="false"-->
    <!--android:drawable="@drawable/back_switch_g"></item>-->

    <item android:state_checked="true">
        <shape android:shape="rectangle">
            <corners android:radius="12dp" />
            <size android:width="50dp" android:height="24dp" />
            <solid android:color="#19AFDB" />
            <stroke android:width="1dp" android:color="#19AFDB" />
        </shape>
    </item>
    <item android:state_checked="false">
        <shape android:shape="rectangle">
            <corners android:radius="12dp" />
            <size android:width="50dp" android:height="24dp" />
            <solid android:color="#9e9e9e" />
            <stroke android:width="1dp" android:color="#9e9e9e" />
        </shape>
    </item>
</selector>